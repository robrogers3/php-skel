{
    "name": "rrogers/php-skel",
    "require": {
        "tightenco/collect": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "psy/psysh": "^0.9.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
        "app/helpers.php"
    ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rob Rogers",
            "email": "robrogers@me.com"
        }
    ]
}
